   "source": [
    "%sql\n",
    "with retailer_name_sim AS (\n",
    "  select\n",
    "    cust_name,\n",
    "    ai_similarity('amazon', cust_name) as sim\n",
    "  from\n",
    "    amitabh_arora_catalog.`demo_vectorization`.retailer_names\n",
    ")\n",
    "select\n",
    "  *\n",
    "from\n",
    "  retailer_name_sim\n",
    "where\n",
    "  sim > 0.9\n",
    "order by\n",
    "  sim desc"
   ]
  }
